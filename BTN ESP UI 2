local ESP = loadstring(game:HttpGet("https://raw.githubusercontent.com/Bomzinho778/Gusi-e/refs/heads/main/BTN%20ESP%20Library"))()
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "Rayfield Example Window",
   Icon = 0,
   LoadingTitle = "Loading BTN ESP",
   LoadingSubtitle = "by Bomzinho",
   Theme = "Amethyst",

   DisableRayfieldPrompts = true,
   DisableBuildWarnings = true,

   ConfigurationSaving = {
      Enabled = true,
      FolderName = BTN ESP,
      FileName = "BTN ESP",
   },

   Discord = {
      Enabled = false
      Invite = "noinvitelink",
      RememberJoins = true
   },

   KeySystem = true,
   KeySettings = {
      Title = "Key System",
      Subtitle = "Buy The Key on SX Store Discord",
      Note = "No method of obtaining the key is provided",
      FileName = "BTNKEY",
      SaveKey = true,
      GrabKeyFromSite = false,
      Key = {"1234", 4321}
   }
})

local Tab = Window:CreateTab("ESP Settings", 4483362458)

local Section = Tab:CreateSection("General Settings")

local Toggle = Tab:CreateToggle({
   Name = "Enable ESP",
   CurrentValue = ESP.Enabled,
   Flag = "Toggle_ESP",
   Callback = function(Value)
       ESP.Enabled = Value
   end,
})

local ColorPicker_BoxOutline = Tab:CreateColorPicker({
    Name = "Box Outline Color",
    Color = ESP.BoxOutlineColor,
    Flag = "ColorPicker_BoxOutline",
    Callback = function(Value)
        ESP.BoxOutlineColor = Value
    end,
})

local ColorPicker_Box = Tab:CreateColorPicker({
    Name = "Box Color",
    Color = ESP.BoxColor,
    Flag = "ColorPicker_Box",
    Callback = function(Value)
        ESP.BoxColor = Value
    end,
})

local ColorPicker_Name = Tab:CreateColorPicker({
    Name = "Name Color",
    Color = ESP.NameColor,
    Flag = "ColorPicker_Name",
    Callback = function(Value)
        ESP.NameColor = Value
    end,
})

local ColorPicker_HealthOutline = Tab:CreateColorPicker({
