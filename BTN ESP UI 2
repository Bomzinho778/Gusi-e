local Slider_TracerThickness = Tab:CreateSlider({
    Name = "Tracer Thickness",
    Range = {1, 10},
    Increment = 1,
    Suffix = "px",
    CurrentValue = ESP.TracerThickness,
    Flag = "Slider_TracerThickness",
    Callback = function(Value)
        ESP.TracerThickness = Value
    end,
})

local Dropdown_BoxType = Tab:CreateDropdown({
   Name = "Box Type",
   Options = {"2D", "Corner Box Esp"},
   CurrentOption = ESP.BoxType,
   Flag = "Dropdown_BoxType",
   Callback = function(Option)
       ESP.BoxType = Option
   end,
})

local Dropdown_TracerPosition = Tab:CreateDropdown({
   Name = "Tracer Position",
